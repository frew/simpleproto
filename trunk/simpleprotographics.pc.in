Cflags: -I${includedir}/@SIMPLEPROTOGRAPHICS_LIBRARY_NAME@-@SIMPLEPROTOGRAPHICS_API_VERSION@ -I${libdir}/@SIMPLEPROTOGRAPHICS_LIBRARY_NAME@-@SIMPLEPROTOGRAPHICS_API_VERSION@/include
